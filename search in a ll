/**
 * Definition for singly-linked list.
 * class ListNode {
 *     public int val;
 *     public ListNode next;
 *     ListNode(int x) { val = x; next = null; }
 * }
 */
public class Solution {
    public int solve(ListNode A, int B) {
        ListNode h = A;
        while(h != null){
            if(h.val == B) return 1;
            h=h.next;
        }
        return 0;
    }
}
