public class Solution {
    public int solve(int[][] A) {
        int n=A.length;
        int sum=0;
        for(int i=0;i<n;i++){
            for(int j=0;j<n;j++){
                int f=(j+1)*(i+1)*(n-j)*(n-i);
                int contr = f*A[i][j];
                sum += contr;
            }
        }
        return sum;
    }
}
